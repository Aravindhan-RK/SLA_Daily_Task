package condition_and_loop;
import java.util.HashSet;
import java.util.Iterator;
import java.util.LinkedHashSet;
import java.util.Set;
import java.util.Scanner;
public class setmethods {
	public static void main(String[] args) {		
		Set<String>names=new HashSet<String>();
		Set<String>friend=new LinkedHashSet<String>();
		Scanner sc=new Scanner(System.in);
		names.add("ram");
		names.add("Selva");
		names.add("mohan");
		names.add("aravind");
		friend.add("james");
		friend.add("raghul");
		names.addAll(friend);
		System.out.println(names);
		System.out.println("Enter the Name:");
		String name=sc.next();
		Iterator<String>ob=names.iterator();
		while(ob.hasNext()) {
		String i=ob.next();
			if(i == name) {
				System.out.println("The names are:"+i);
			}
			else {
				names.add(name);
				System.out.println("The added names:"+names);
				break;
			}
		}
	}
}
